//Nullish Colescing Operator ✅

//• para o js valores como 0, '', [], false, undefined, null => são valores falsy

//  (idade ?? 'Não informado') => Nullish Colescing Operator;

//Objetos ✅

const user = {
  name: "Diego",
  idade: 12,
  address: {
    street: "rua tal",
    number: 156,
  },
};

Object.keys(user); // retorna todas as chaves do objeto

//Desestruturação ✅

// const address = user.address;  ao inves de repetir o address, se faz uma desestruturação

// ficaria dessa forma

// const { address, idade: age ( acabei de mudar o nome da variavel)} =  user;

//Rest operator ✅

const { name, ...rest } = user; //pego o resto apartir do name, menos o name

//Short syntax ✅

// const name = 'Diego';
// const age = 12;

// const user = {
//   name,
//   age,
// }
